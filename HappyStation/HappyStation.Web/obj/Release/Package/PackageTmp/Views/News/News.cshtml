@model NewsViewModel
@{
    ViewBag.Title = Model.Title;
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div id="main" class="container">
    <div class="row">

        @Html.Action("PreviewList", "News")

        <div class="9u skel-cell-important">
            <section>
                <header>
                    <h2>@Model.Title</h2>
                </header>
                <div>
                    @Html.Raw(Model.Text)
                </div>
            </section>
        </div>
    </div>
</div>